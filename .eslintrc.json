{
    "env": {
        "browser": true,
        "es2021": true,
        "qunit": true
    },
    "extends": [
        "airbnb-base",
        "plugin:qunit/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-param-reassign": [2, { "props": false }],
        "prefer-arrow-callback": "off"
    }
}
